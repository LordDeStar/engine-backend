{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/engine-backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/engine-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/engine-backend",
  "projects": {
    "engine-backend": {
      "type": "application",
      "root": "apps/engine-backend",
      "entryFile": "main",
      "sourceRoot": "apps/engine-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/engine-backend/tsconfig.app.json"
      }
    },
    "project-service": {
      "type": "application",
      "root": "apps/project-service",
      "entryFile": "main",
      "sourceRoot": "apps/project-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/project-service/tsconfig.app.json"
      }
    },
    "projects": {
      "type": "application",
      "root": "apps/projects",
      "entryFile": "main",
      "sourceRoot": "apps/projects/src",
      "compilerOptions": {
        "tsConfigPath": "apps/projects/tsconfig.app.json"
      }
    },
    "rest_api": {
      "type": "application",
      "root": "apps/rest_api",
      "entryFile": "main",
      "sourceRoot": "apps/rest_api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rest_api/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}